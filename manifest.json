{
  "manifest_version": 2,
  "name": "PatientText",
  "version": "1.0",
  "permissions": [
	"activeTab",
    "tabs", "<all_urls>"
  ],
  "background": {
    "scripts": ["myscript.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Translate medical abbreviations to simple text",
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "termlist.txt"
	]
}